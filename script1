end 
end 
end
)


tgls:Toggle(
    "Anti-Afk",
    false,
    function(bool)
        _G.AntiAFK = bool
        if not _G.AntiAFK_Connection then
            _G.AntiAFK_Connection = player.Idled:Connect(function()
                if _G.AntiAFK then
                    VirtualUser:CaptureController()
                    if VirtualUser.ClickButton2 then
                        VirtualUser:ClickButton2(Vector2.new(0,0))
                    else
                        VirtualUser:Button2Down(Vector2.new(0,0))
                        task.wait(0.08)
                        VirtualUser:Button2Up(Vector2.new(0,0))
                    end
                end
            end)
        end
    end
)

