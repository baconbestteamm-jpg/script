local Players = game:GetService("Players")
local VirtualUser = game:GetService("VirtualUser")
local player = Players.LocalPlayer

local DiscordLib =
    loadstring(game:HttpGet "https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/discord")()

local win = DiscordLib:Window("discord library")

local serv = win:Server("Genovia High Auto Farm", "")


local tgls = serv:Channel("Toggles")

tgls:Toggle( "Auto-Farm", false, function(bool) _G.AutoFarm = bool while _G.AutoFarm do task.wait(0.5) 
if game.Players.LocalPlayer.PlayerGui.MoodGUI.ImageLabel.TimeLabel.Text:find("8:00") and game.Players.LocalPlayer.PlayerGui.MoodGUI.ImageLabel.TimeLabel.Text:find("am") and game:GetService("Players").LocalPlayer.PlayerGui.MoodGUI.ImageLabel.DayLabel.Text == "A" then game.ReplicatedStorage.Remotes.Functions.TeleportToDestination:InvokeServer(workspace.Teleports.Scheduled, "Fencing") end 
