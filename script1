if game.Players.LocalPlayer.PlayerGui.MoodGUI.ImageLabel.TimeLabel.Text:find("10:00") and game.Players.LocalPlayer.PlayerGui.MoodGUI.ImageLabel.TimeLabel.Text:find("am") and game:GetService("Players").LocalPlayer.PlayerGui.MoodGUI.ImageLabel.DayLabel.Text == "B" then game.ReplicatedStorage.Remotes.Functions.TeleportToDestination:InvokeServer(workspace.Teleports.Scheduled, "Mathematics") end 
if game.Players.LocalPlayer.PlayerGui.MoodGUI.ImageLabel.TimeLabel.Text:find("11:00") and game.Players.LocalPlayer.PlayerGui.MoodGUI.ImageLabel.TimeLabel.Text:find("am") and game:GetService("Players").LocalPlayer.PlayerGui.MoodGUI.ImageLabel.DayLabel.Text == "B" then game.ReplicatedStorage.Remotes.Functions.TeleportToDestination:InvokeServer(workspace.Teleports.Scheduled, "Music") task.wait(170) end 
if game.Players.LocalPlayer.PlayerGui.MoodGUI.ImageLabel.TimeLabel.Text:find("2:00") and game.Players.LocalPlayer.PlayerGui.MoodGUI.ImageLabel.TimeLabel.Text:find("pm") and game:GetService("Players").LocalPlayer.PlayerGui.MoodGUI.ImageLabel.DayLabel.Text == "B" then game.ReplicatedStorage.Remotes.Functions.TeleportToDestination:InvokeServer(workspace.Teleports.Scheduled, "Art") end 
if game.Players.LocalPlayer.PlayerGui.MoodGUI.ImageLabel.TimeLabel.Text:find("4:00") and game.Players.LocalPlayer.PlayerGui.MoodGUI.ImageLabel.TimeLabel.Text:find("pm") and game:GetService("Players").LocalPlayer.PlayerGui.MoodGUI.ImageLabel.DayLabel.Text == "B" then game.ReplicatedStorage.Remotes.Functions.TeleportToDestination:InvokeServer(workspace.Teleports.Scheduled, "Ballroom") end 
if game.Players.LocalPlayer.PlayerGui.MoodGUI.ImageLabel.TimeLabel.Text:find("8:00") and game.Players.LocalPlayer.PlayerGui.MoodGUI.ImageLabel.TimeLabel.Text:find("am") and game:GetService("Players").LocalPlayer.PlayerGui.MoodGUI.ImageLabel.DayLabel.Text == "C" then game.ReplicatedStorage.Remotes.Functions.TeleportToDestination:InvokeServer(workspace.Teleports.Scheduled, "Alchemy") end 
